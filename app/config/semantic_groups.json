{
  "semantic_groups": {
    "basic_info": {
      "title": "基本信息",
      "priority": 1,
      "keywords": {
        "name": ["姓名", "真实姓名", "用户名", "名字", "全名", "name", "真名", "称呼"],
        "gender": ["性别", "gender", "男女", "性別"],
        "phone": ["手机", "电话", "联系电话", "手机号", "联系方式", "phone", "mobile", "tel", "电话号码", "手机号码"],
        "email": ["邮箱", "邮件", "email", "e-mail", "电子邮件", "电子邮箱", "联系邮箱"],
        "birth": ["出生", "生日", "出生日期", "生年月日", "birthday", "birth", "年龄相关", "生日日期"],
        "id_card": ["身份证", "证件号", "身份证号", "id", "证件", "身份证号码", "个人证件"],
        "address": ["地址", "住址", "居住地", "联系地址", "详细地址", "address", "location", "现住址", "家庭住址"],
        "nationality": ["国籍", "民族", "nationality", "ethnicity", "籍贯"]
